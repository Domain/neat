module ufcs;

macro import cx.macros.assert;

import helpers;

struct S
{
    int i;
    int bar() { return i; }
}

int getI(S s) { return s.i; }
int bar(S s) { return 6; }

void main() {
    S s = S(5);
    assert(s.getI == 5);
    assert(s.getI() == 5);
    assert(s.bar() == 5);
    assert(s.bar == 5);

    "Success!".print;
}
