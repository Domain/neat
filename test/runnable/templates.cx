module templates;

import helpers;

template LinkedList(T) {
    class LinkedList {
        this() { }
        LinkedList next;
        T value;
    }
}

void main() {
    print("Test...");
    auto ll = new LinkedList!int();
    ll.next = new LinkedList!int();
    ll.value = 5;
    ll.next.value = 6;
    print("done.");
}
