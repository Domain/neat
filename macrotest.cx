module macrotest;

macro import cx.macros.assert;

extern(C) void print(char[]);

class A {
    this() { }
}

void main(string[] args) {
    print("macrotest");
    A a = new A;
    assert(a); // this would not have worked before, because objects are not bools
    assert a; // this also not
    print("- success");
}
