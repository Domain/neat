void main() {
    assert(ack(3, 6) == 509);
    assert(whiletest(5) == 32);
}

int ack(int m, int n) {
    if (m == 0) { n = n + 1; return n; }
    if (n == 0) { int m1 = m - 1; return ack(m1, 1); }
    int m1 = m - 1; int n1 = n - 1;
    return ack(m1, ack(m, n1));
}

int whiletest(int k) {
    int i = 1;
    while (k > 0) {
        i = i * 2;
        k = k - 1;
    }
    return i;
}

